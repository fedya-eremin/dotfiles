 ;(package-initialize)

(tool-bar-mode 0)
(menu-bar-mode 0)
(scroll-bar-mode 0)
(setq custom-file "~/dotfiles/emacs/custom.el")
(setq backup-directory-alist '(("." . "/tmp")))
